<% users.each do |user| %>
  <div class="row user-listing-<%= user.user_id %>">
    <div class="col-md-6"><%= user.name %></div>
    <div class="col-md-6"> 
      <%=link_to 'Accept Request', '#' ,class: "btn btn-success btn-sm accept-request", data: {user_id: user.user_id} %>
    </div>
  </div>
<% end %>